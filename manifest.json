{
  "manifest_version": 3,
  "name": "Chrome Password Locker",
  "version": "1.4",
  "description": "Khoá Chrome!! by qckhanh195@gmail.com",
  "permissions": ["storage", "tabs", "scripting", "webNavigation"],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": ["background.png", "background_option.png", "icons/icon128.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Khóa trình duyệt",
    "default_popup": "popup.html"
  }
}

